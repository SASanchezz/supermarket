<Window x:Class="supermarket.Windows.ManagerMenu.EmployeeWindows.ManagerAddEmployeeWindow" Background="LightSlateGray"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:vm="clr-namespace:supermarket.ViewModels.ManagerMenu.EmployeesViewModels"
        xmlns:v="clr-namespace:supermarket.Views.ManagerMenu.EmployeesViews"
        xmlns:local="clr-namespace:supermarket.Windows.ManagerMenu.EmployeeWindows"
        d:DataContext="{d:DesignInstance Type=vm:ManagerAddEmployeeWindowViewModel}"
        mc:Ignorable="d"
        Title="AddUserWindow" Height="450" Width="800"
        Content="{Binding ViewModel, Mode=OneWay}" IsEnabled="{Binding IsEnabled, Mode=OneWay}" ResizeMode="NoResize">
    <Window.Resources>
        <DataTemplate DataType="{ x:Type vm:ManagerAddEmployeeViewModel}">
            <v:ManagerAddEmployeeView/>
        </DataTemplate>
    </Window.Resources>
</Window>
