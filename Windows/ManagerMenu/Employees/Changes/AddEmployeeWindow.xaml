<Window x:Class="supermarket.Windows.ManagerMenu.Employees.Changes.AddEmployeeWindow" Background="LightSlateGray"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:views="clr-namespace:supermarket.Views.ManagerMenu.Employees.Changes"
        xmlns:VM="clr-namespace:supermarket.ViewModels.ManagerMenu.Employees.Changes"
        xmlns:local="clr-namespace:supermarket.Windows.ManagerMenu.Employees.Changes"
        d:DataContext="{d:DesignInstance Type=VM:AddEmployeeWindowVM}"
        mc:Ignorable="d"
        Title="AddUserWindow" Height="450" Width="800"
        Content="{Binding ViewModel, Mode=OneWay}" IsEnabled="{Binding IsEnabled, Mode=OneWay}" ResizeMode="NoResize">
    <Window.Resources>
        <DataTemplate DataType="{ x:Type VM:AddEmployeeVM}">
            <views:AddEmployeeView/>
        </DataTemplate>
    </Window.Resources>
</Window>
