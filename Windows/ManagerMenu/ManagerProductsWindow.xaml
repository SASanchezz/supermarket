<Window x:Class="supermarket.Windows.ManagerMenu.ManagerProductsWindow" Background="LightSlateGray"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:v ="clr-namespace:supermarket.Views.ManagerMenu"
        xmlns:vm ="clr-namespace:supermarket.ViewModels.ManagerMenu"
        xmlns:local="clr-namespace:supermarket.Windows.ManagerMenu"
        mc:Ignorable="d"
        Title="MainProductWindow" Height="450" Width="800"
        Content="{Binding ViewModel, Mode=OneWay}" IsEnabled="{Binding IsEnabled, Mode=OneWay}">

    <Window.Resources>
        <DataTemplate DataType="{ x:Type vm:ManagerProductsViewModel}">
            <v:ManagerProductsView/>
        </DataTemplate>
    </Window.Resources>
    
</Window>
